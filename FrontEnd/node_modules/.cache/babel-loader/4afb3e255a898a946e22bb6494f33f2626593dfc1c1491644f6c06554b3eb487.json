{"ast":null,"code":"import modificaDatiUtente from \"@/components/modificaDatiUtente.vue\";\nimport modificaDescrizione from \"@/components/modificaDescrizione.vue\";\nimport { mapState } from \"vuex\";\nimport format from 'date-fns/format';\nexport default {\n  components: {\n    modificaDatiUtente,\n    modificaDescrizione\n  },\n  data() {\n    return {\n      euro: new Intl.NumberFormat('en-DE', {\n        style: 'currency',\n        currency: 'EUR'\n      })\n    };\n  },\n  methods: {\n    formattedDate(date) {\n      return format(new Date(date), 'dd/M/YYY');\n    }\n  },\n  computed: {\n    ...mapState({\n      utente: 'datiUtente',\n      profilo: 'profiloUtente'\n    })\n  }\n};","map":{"version":3,"mappings":"AA+JA;AACA;AACA;AACA;AAEA;EACAA;IAAAC;IAAAC;EAAA;EACAC;IACA;MACAC,MACA;QACAC;QACAC;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IAEA;MACAC;MACAC;IACA;EACA;AACA","names":["components","modificaDatiUtente","modificaDescrizione","data","euro","style","currency","methods","formattedDate","computed","utente","profilo"],"sourceRoot":"src/views","sources":["modificaDatiPersonali.vue"],"sourcesContent":["<template>\n    <v-container >\n        <v-card class=\"elevation-3\">\n        <v-window class=\"spacing-playground pa-6\">\n            <v-row>\n                <v-col>\n                    \n                    <v-row>\n                        <h2>Dati personali:</h2> \n                    </v-row>\n                    <v-row>\n                        <v-divider></v-divider>\n                    </v-row>\n                    <v-row>\n                        <v-col cols=\"4\">\n                            <span class=\"font-weight-regular\">Nome:</span>\n                        </v-col>\n                        <v-col>\n                            <span class=\"font-weight-light\"> {{ utente.nome }} </span> \n                        </v-col>\n                    </v-row>\n                    <v-row>\n                        <v-col cols=\"4\">\n         